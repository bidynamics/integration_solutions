                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">--Retrieve Current Data and Log File Sizes for All Databases on the Server
SELECT 
	GETDATE() AS [captureDate]
	, @@SERVERNAME AS [serverName]
	, instance_name AS [databaseName]
	, SUM(
		CASE
			WHEN counter_name = 'Data File(s) Size (KB)'
			THEN cntr_value
		END
	) AS 'dataSizeInKB'
	, SUM(
		CASE
			WHEN counter_name = 'Log File(s) Size (KB)'
			THEN cntr_value
		END
	) AS 'logSizeInKB'
FROM 
	sys.dm_os_performance_counters
WHERE 
		counter_name IN ('Data File(s) Size (KB)','Log File(s) Size (KB)')
	AND instance_name &lt;&gt; '_Total'
GROUP BY 
	instance_name
;</property>
